<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wanglongxiang.mychat.mapper.CronyGroupMapper">
    <insert id="save" useGeneratedKeys="true" keyProperty="id">
        insert into crony_group (userid, cronygroupname)
        values (#{userId},#{cronyGroupName});
    </insert>
</mapper>